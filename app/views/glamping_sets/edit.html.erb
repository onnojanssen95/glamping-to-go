<%= form_for(@glamping_set) do |f| %>
 <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :total_occupancy %>
  <%= f.number_field :total_occupancy %>
  <%= f.label :price_per_night %>
  <%= f.number_field :price_per_night %>
  <%= f.input :photo, as: :file %>
 <%= f.submit %>
<% end %>
